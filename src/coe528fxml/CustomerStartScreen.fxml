        </columns>
      </TableView>
      <Button fx:id="buy" layoutX="45.0" layoutY="342.0" mnemonicParsing="false" onAction="#buyBook" prefHeight="25.0" prefWidth="73.0" text="Buy" />
      <Button fx:id="buy_redeem" layoutX="220.0" layoutY="342.0" mnemonicParsing="false" onAction="#buyRedeemBook" prefHeight="25.0" prefWidth="172.0" text="Redeem Points and Buy" />
      <Button layoutX="459.0" layoutY="342.0" mnemonicParsing="false" onAction="#changeToLoginScreen" prefHeight="25.0" prefWidth="73.0" text="Logout" />
